<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="UI Playwright Suite" parallel="tests">
    <listeners>
        <listener class-name="listeners.AllurePlaywrightListener"/>
        <listener class-name="org.example.listeners.AllureListener"/>
    </listeners>
    <test name="Power of Playwright">
        <classes>
            <class name="InterceptAndModifyResponseBodyTest"/>
            <class name="VideoTest"/>
            <class name="OfflineErrorTest"/>
            <class name="TracingTest"/>
            <class name="github.GitHubLoginWithoutStoredSessionTest"/>
<!--            <class name="github.GitHubLoginByLoadingStoredSessionTest"/>-->
        </classes>
    </test>
</suite>